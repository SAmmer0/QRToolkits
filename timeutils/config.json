{
    "calendar":{
            // main path of calendar database
        "calendar_db_path": "~/Documents/TradingCalendarDB",
        // sub-calendars' relative pathes
        "data": {
            // stock market calendar
            "stock": "StockCalendar"
        }
    },
    "log": {
        // log is save to file, otherwise log will be printed
        "log_to_file": true,
        // log path where the log file is saved, starting with "./" means relative path
        // Warning: in configuration file separator is "/"
        "log_path": "./timeutils.log",
        // log level
        "log_level": "DEBUG",
        // log format
        "format": "%(asctime)s %(levelname)s %(filename)s-%(lineno)s: %(message)s",
        // date format
        "date_format": "%Y-%m-%d %H:%M:%S"
    }

}